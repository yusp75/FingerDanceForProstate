<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yushealth.prostate.fingerdanceforprostate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service android:name="MusicService" android:enabled="true" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".BodyActivity" />
        <activity android:name=".BreatheActivity" />
        <activity android:name=".FingerActivity" />
        <activity android:name=".ProfileActivity" />

        <receiver android:name=".AlarmReceiver" />

        <activity android:name=".SettingClockActivity" />
        <activity android:name=".SettingLevelActivity" />
        <activity android:name=".SettingMusicActivity" />
        <activity android:name=".MusicFileListActivity"></activity>
    </application>

</manifest>